@model MVCOnlineTicariOtomasyon.Models.Siniflar.Personel
@{
    ViewBag.Title = "PersonelEkle";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<br />
<h2>Yeni Personel Sayfası</h2>
<br />

<from class="from-gruop" method="post">
    @Html.HiddenFor(x=>x.PersonelID)
    @Html.LabelFor(x=>x.PersonelAd)
    @Html.TextBoxFor(x=>x.PersonelAd,new {@class="from-control"})
  
    @Html.LabelFor(x=>x.PersonelSoyad)
    @Html.TextBoxFor(x=>x.PersonelSoyad, new {@class="from-control"})
    
    @Html.LabelFor(x=>x.PersonelGorsel)
    @Html.TextBoxFor(x => x.PersonelGorsel, new {@class="from-control"})
  
    @Html.LabelFor(x=>x.Departman)
    @Html.DropDownListFor(x => x.Departmanid, (List<SelectListItem>)ViewBag.dgr1, new {@class="from-conrol"})
    <br />
    <br />
    <button class="btn btn-info">Kaydet</button>
</from>
